package patterns;
/*
*****
*****
*****
*****
*****
 */

public class Pattern1 {
    public static void main(String[] args) {
        pattern1(5);
    }
    static void pattern1(int n){
        //for outer loop
        for (int row = 1; row <= n ; row++) {
            //for every row, inner loop(col)
            for (int col = 1; col <= n ; col++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
