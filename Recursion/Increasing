package Recursion;

public class Increasing {
    public static void main(String[] args) {
        int n = 5;
        printIncreasing(n);
    }
    static  void printIncreasing(int n) {
        //base condition
        if (n==1){
            System.out.println(n);
            return;
        }
        //recursive-work
        printIncreasing(n-1);
        //self-work
        System.out.println(n);
    }
}
