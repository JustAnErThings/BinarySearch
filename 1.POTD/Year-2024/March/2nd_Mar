//QuestionLink: https://www.geeksforgeeks.org/problems/first-element-to-occur-k-times5150/1

class Solution
{
    public int firstElementKTime(int n, int k, int[] a) { 
         LinkedHashMap<Integer, Integer> map = new LinkedHashMap<>();
        
        for(int i = 0; i < n; i++) {
            map.put(a[i], map.getOrDefault(a[i], 0)+1);
            if(map.get(a[i]) == k)
                return a[i];
        }
        
        return -1;
    } 
}
