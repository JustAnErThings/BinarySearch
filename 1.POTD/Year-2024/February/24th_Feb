//QuestionLink: https://www.geeksforgeeks.org/problems/maximum-sum-problem2211/1

//Java Code

class Solution
{
    public int maxSum(int n) 
    { 
         if (n <= 1) {
            return n;
        }
        
        int partsSum = maxSum(n / 2) + maxSum(n / 3) + maxSum(n / 4);

        return Math.max(n, partsSum);
    } 
}
