//Question Link: https://www.geeksforgeeks.org/problems/reach-a-given-score-1587115621/1

//Java Code

class Geeks {
    public long count(int n) {
         int[] dp = new int[n + 1];

        dp[0] = 1;

        int[] scores = {3, 5, 10};

        for (int score : scores) {
            for (int i = score; i <= n; i++) {
                dp[i] += dp[i - score];
            }
        }

        return dp[n];
    }
}
