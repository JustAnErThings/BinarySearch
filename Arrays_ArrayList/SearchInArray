package Arrays;

public class SearchInArray {
    public static void main(String[] args) {
        int[] arr = {3,7,2,11,4,23,14,18,19,16,33};
        int target = 18;
        System.out.println(search(arr,target));
    }

     static int search(int[] arr, int target) {
         for (int i = 0; i < arr.length; i++) {
             if (arr[i] == target){
                 return i;
             }
         }
         return -1;
    }
}
