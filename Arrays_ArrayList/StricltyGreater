package Arrays;

public class StricltyGreater {
    public static void main(String[] args) {
        int[] arr = {3,5,7,1,8,9};
        int target = 5;
        System.out.println(greater(arr,target));
    }

     static int greater(int[] arr, int target) {
        int count = 0;
         for (int i = 0; i < arr.length; i++) {
             if (arr[i] > target){
                 count++;
             }
         }
         return count;
    }
}
