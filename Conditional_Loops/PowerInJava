package JAVA.Conditional;

import java.util.Scanner;

public class PowerInJava {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();

        System.out.println(a +" power "+b +" is: " +Math.pow(a,b));
    }
}
